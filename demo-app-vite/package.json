{
	"name": "mockaton-demo-app-vite",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"mockaton": "mockaton --port=4040",
		"start": "BACKEND=http://localhost:4040 vite",
		"pixaton": "node --test --import=./pixaton-tests/_setup.js --experimental-test-isolation=none \"./pixaton-tests/**/*.test.js\"",
		"outdated": "npm outdated --parseable | awk -F: '{ printf \"npm i %-30s ;# %s\\n\", $4, $2 }'",
		"build": "vite build"
	},
	"dependencies": {
		"react": "19.2.3",
		"react-cookie": "8.0.1",
		"react-dom": "19.2.3"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "5.1.2",
		"mockaton": "12.1.0",
		"pixaton": "1.1.3",
		"vite": "7.3.1"
	},
	"engines": {
		"node": ">23.6"
	}
}
